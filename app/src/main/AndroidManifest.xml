<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycard">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyCard">
        <activity android:name=".Activities.Lost_Card"></activity>
        <activity android:name=".Activities.ShowQR" />
        <activity android:name=".Activities.Security" />
        <activity android:name=".Activities.Deanship" />
        <activity android:name=".Activities.Payment_info" />
        <activity android:name=".Activities.Fine_payment_alert" />
        <activity android:name=".Activities.Forget_Card" />
        <activity android:name=".Activities.Student" />
        <activity android:name=".Activities.Login" />
        <activity android:name=".Activities.UserChoice" />
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>