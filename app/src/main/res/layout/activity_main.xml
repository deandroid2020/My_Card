    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/main"
    tools:context=".Activities.MainActivity">

    <include
        android:id="@+id/toto"
        layout="@layout/mybar"
        />

    <Space
